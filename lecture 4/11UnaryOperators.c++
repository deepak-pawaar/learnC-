/*
+ Unary plus
- Unary minus
++ increment Operator   a++, ++a
-- decrement Operator   a--, --a
! Logical complement Operator   !true = false, !1=0
*/
#include<iostream>
using namespace std;
int main(){
    int a=6;
    cout<<(a++)<<endl;
    int b=5;
    cout<<(--b)<<endl;
    return 0;
}

